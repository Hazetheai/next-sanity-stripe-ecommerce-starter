{
  "name": "sanity-template-nextjs-ecommerce",
  "version": "1.0.0",
  "private": true,
  "description": "Sanity.io and Next.js Ecommerce Starter template",
  "scripts": {
    "dev": "concurrently \"tsc --watch\" \"next\"",
    "dev-s": "concurrently \"tsc --watch\" \"next\" \"cd studio && sanity start\"",
    "build": "echo 'Building Sanity to public/cms-studio' && cd studio && sanity build ../public/cms-studio -y && cd .. && next build",
    "build-no-studio": "next build",
    "start": "vercel dev",
    "postinstall": "lerna bootstrap",
    "check-template": "sanity-template check"
  },
